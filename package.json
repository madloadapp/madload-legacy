  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^6.1.0",
    "@types/jest": "^24.9.1",
    "@types/koa": "^2.11.2",
    "@types/koa-bodyparser": "^4.3.0",
